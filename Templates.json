[
	{
		"type": "container",
		"title": "SuiteCRM PHP 7.1",
 		"description": "SuiteCRM PHP7.1 container deploy",
 		"categories": ["crm"],
		"logo": "https://raw.githubusercontent.com/salesagility/SuiteCRM/master/themes/SuiteP/images/sugar_icon.ico",
		"image": "hub.salesagility.com/internal/docker/devphp/7.1:v1.1",
		"registry": "hub.salesagility.com",
		"network": "staging",
		"ports": ["80/tcp"],
		"env": [ {
			"name": "VIRTUAL_HOST",
			"label": "Container Domain (test.example.com)"
						 },
						 {
			"name": "HOST_SITE",
			"label": "Instance Name (production)"
		         }
				   ],
		"volumes": [ {
			"container": "/var/www",
			"bind": "/app/{instance}"
		           } ],
		"interactive": true,
 		"restart_policy": "unless-stopped",
		"platform": "linux"
	}

