	{
		"title": "MariaDB",
		"description": "MariaDB container deployment",
		"categories": ["database"],
		"platform": "linux",
		"restart_policy": "unless-stopped",
		"logo": "https://cloudinovasi.id/assets/img/logos/mariadb.png",
		"image": "mariadb:latest",
		"env": [{
				"name": "MYSQL_ROOT_PASSWORD",
				"label": "Root password"
			   }],
		"ports":
			["3306/tcp"],
		"volumes": [ {
			"container": "/var/lib/mysql",
